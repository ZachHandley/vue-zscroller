{
  "name": "vue-zscroller",
  "type": "module",
  "version": "1.0.0",
  "description": "A modern fork of vue-virtual-scroller with TypeScript and @vueuse/core integration",
  "author": {
    "name": "Guillaume Chau",
    "email": "guillaume.b.chau@gmail.com"
  },
  "maintainer": {
    "name": "Zach Handley",
    "email": "zachhandley@gmail.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/ZachHandley/zvue-virtual-scroller#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZachHandley/zvue-virtual-scroller.git"
  },
  "bugs": {
    "url": "https://github.com/ZachHandley/zvue-virtual-scroller/issues"
  },
  "keywords": [
    "vue",
    "vuejs",
    "plugin"
  ],
  "sideEffects": true,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/vue-zscroller.esm.mjs",
      "require": "./dist/vue-zscroller.umd.js"
    }
  },
  "main": "dist/vue-zscroller.umd.js",
  "module": "dist/vue-zscroller.esm.mjs",
  "unpkg": "dist/vue-zscroller.min.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "prepublishOnly": "pnpm run lint && pnpm run build",
    "dev": "vite build --watch",
    "lint": "cd ../../ && pnpm run lint"
  },
  "peerDependencies": {
    "vue": "^3.4.0"
  },
  "dependencies": {
    "@vueuse/core": "^10.11.1"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.6.2",
    "vite": "^5.4.10",
    "vite-plugin-dts": "^4.5.4",
    "vue": "^3.5.13",
    "vue-tsc": "^2.2.8"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "publishConfig": {
    "access": "public"
  }
}
